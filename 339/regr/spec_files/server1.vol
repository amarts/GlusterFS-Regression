volume posix
 type storage/posix
 option directory /share/tickets/339/regr/export/export1
end-volume

volume locks
 type features/locks
 subvolumes posix
end-volume

volume brick
 type performance/io-threads
 option thread-count 8
 subvolumes locks
end-volume

volume server
 type protocol/server
 option transport-type tcp
 option auth.addr.brick.allow *
 subvolumes brick
end-volume