volume posix
  type storage/posix
  option directory /jbod/regr/313/export1
end-volume

volume locks
  type features/locks
  subvolumes posix
end-volume

volume brick
  type performance/io-threads
  option thread-count 8
  subvolumes locks
end-volume

volume server
  type protocol/server
  option transport-type tcp
  option listen-port 9350
  option auth.addr.brick.allow *
  subvolumes brick
end-volume
